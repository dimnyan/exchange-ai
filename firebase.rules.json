{
  "rules": {
    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid",
        "positions": {
          "$positionId": {
            ".validate": "newData.hasChildren(['symbol', 'entry', 'size', 'leverage'])"
          }
        }
      }
    }
  }
}